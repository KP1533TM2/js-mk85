Test register modes 1x and 2x


8000:	mov #8008, r5
8004:	jmp (r5)			; test register deferred
8006:	---
8008:	mov #8010, r5
800c:	jmp (r5)+			; test autoincrement
800e:	---
8010:	br	#8020
8012:	br	#8026
8014:	br	#802c

8016:	br	#8000

8020:	mov #5500, r0
8024:	br	#800c

8026:	mov #0550, r0
802a:	br	#800c

802c:	mov #0055, r0
8030:	br	#800c

var testJMP_1x_and_2x = {
	0x0000:	[0x8000, 0x0500],
	0x8000: [0x15c5, 0x8008, 0x004d],
	0x8008: [0x15c5, 0x8010, 0x0055],
	0x8010: [0x0107, 0x0109, 0x010b, 0x01f4],
	0x8020: [0x15c0, 0x5500, 0x01f3],
	0x8026: [0x15c0, 0x0550, 0x01f0],
	0x802c: [0x15c0, 0x0055, 0x01ed]
};

