Addr mode:	Desc:						With R1:					can address with
0x			register					----		null			----
1x			register deferred			JMP (R1)	IP <= R1		memAddr
2x			autoincrement				JMP (R1)+	IP <= R1		memAddr
3x			autoincrement deferred		JMP @(R1)+	IP <= (R1)		.ru()
4x			autodecrement				JMP -(R1)	IP <= R1		memAddr
5x			autodecrement deferred		JMP @-(R1)	IP <= (R1)		.ru()
6x			index						JMP X(R1)	IP <= R1+X		memAddr
7x			index deferred				JMP @X(R1)	IP <= (R1+X)	.ru()


8000:	15c5 		mov		#8006, r5
8002:	8006
8004:				mov		#8020


8004:	004d		jmp		(r5)
8006:				mov		#8010, r4			
8010:	
